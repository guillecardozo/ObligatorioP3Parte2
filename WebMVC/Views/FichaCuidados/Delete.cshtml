@model ObligatorioP3.EntidadesNegocio.FichaCuidados

@{
    ViewData["Title"] = "Eliminar Ficha de Cuidados";
}


@if (ViewBag.Error != null)
{
    <div class="alert alert-danger" role="alert">
        @ViewBag.Error
    </div>
}

@if (ViewBag.Ok != null)
{
    <div class="alert alert-success" role="alert">
        @ViewBag.Ok
    </div>
}

<h1>Eliminar</h1>

<div>
    <h4>Ficha de Cuidados</h4>
    <hr />

    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FrecuenciaRiego)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FrecuenciaRiego)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TipoIluminacion)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TipoIluminacion)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Temperatura)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Temperatura)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver al Listado</a>
    </form>
</div>
